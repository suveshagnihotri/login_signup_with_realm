<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StyledView">
        <attr name="tint" format="reference|color" />
        <attr name="font" format="enum">
            <enum name="light" value="0" />
            <enum name="normal" value="1" />
            <enum name="medium" value="2" />
            <enum name="semibold" value="3" />
            <enum name="bold" value="4" />
        </attr>
        <attr name="leftVector" format="reference"/>
        <attr name="rightVector" format="reference"/>
        <attr name="topVector" format="reference"/>
        <attr name="bottomVector" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ShowHidePasswordEditText">
        <attr name="drawable_hide" format="integer"/>
        <attr name="drawable_show" format="integer"/>
        <attr name="tint_color" format="color"/>
        <attr name="additionalTouchTargetSize" format="dimension"/>
    </declare-styleable>

</resources>